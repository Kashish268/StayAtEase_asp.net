
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>SuperAdmin</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/WebApplication1.styles.css" asp-append-version="true" />
    <link rel="shortcut icon" href="\assets\StayAtEase.png">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    @* for review slider *@


</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a href="/" class="navbar-brand">
                    <img src="\assets\StayAtEase.png" alt="Logo" class="nav-logo"> <!-- Add your logo image -->
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-end">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link @(ViewData["ActivePage"] == "Super_AdminDashboard" ? "active" : "")"
                               asp-controller="Super_Admin" asp-action="Super_AdminDashboard">
                                Dashboard
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link @(ViewData["ActivePage"] == "Total_User" ? "active" : "")"
                               asp-controller="Super_Admin" asp-action="Total_User">
                                Tenants
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link @(ViewData["ActivePage"] == "Total_Admin" ? "active" : "")"
                               asp-controller="Super_Admin" asp-action="Total_Admin">
                                Property Owners
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link @(ViewData["ActivePage"] == "Reviews" ? "active" : "")"
                               asp-controller="Super_Admin" asp-action="Reviews">
                                Reviews
                            </a>
                        </li>
                        @* <li class="nav-item"> *@
                        @*     <a class="nav-link @(ViewData["ActivePage"] == "WishList" ? "active" : "")" asp-area="" asp-controller="Home" asp-action="WishList">My Listing</a> *@
                        @* </li> *@
                        @* <li class="nav-item"> *@
                        @*     <a class="nav-link @(ViewData["ActivePage"] == "WishList" ? "active" : "")" asp-area="" asp-controller="Home" asp-action="WishList">Reviews</a> *@
                        @* </li> *@
                        @* <li class="nav-item"> *@
                        @*     <button class="btn btn-primary" id="openLoginModal">Login</button> *@
                        @* </li> *@
                    </ul>


                </div>
            </div>
        </nav>
    </header>
    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)

    <!-- Login Modal (always present in the layout) -->
    @await Html.PartialAsync("_Footer")
    <!-- Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>

    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>


</body>
</html>
